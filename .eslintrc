{
  "root": true,
  "rules": {
    "curly": [2, "multi-line"],
    "dot-notation": 0,
    "eqeqeq": 2,
    "eol-last": 0,
    "handle-callback-err": [2, "^err"],
    "indent": [2, 2],
    "key-spacing": 0,
    "keyword-spacing": [2, {
      "after": true,
      "before": true
    }],
    "linebreak-style": [2, "unix"],
    "no-console": 2,
    "no-debugger": 2,
    "no-mixed-requires": 0,
    "no-multi-spaces": 0,
    "no-new": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "quotes": [2, "single"],
    "react/jsx-curly-spacing": [2, "always"],
    "semi": [2, "always"],
    "semi-spacing": 0,
    "space-in-parens": [2, "never"],
    "strict": 0,
    "no-unused-vars": 2,
    "no-undef": 2
  },
  "env": {
    "node": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": ["react"]
}

